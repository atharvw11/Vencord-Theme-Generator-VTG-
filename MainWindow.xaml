<Window x:Class="VencordThemeGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:winui="using:Microsoft.UI.Xaml.Controls"
        xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
        Title="Vencord Link Generator" Height="450" Width="525">
    <Grid>
        <muxc:StackLayout Margin="20">
            <TextBlock Text="Enter your links:" Margin="0,0,0,10"/>
            <winui:TextBox x:Name="LinkTextBox1" Margin="5" PlaceholderText="Enter link 1" />
            <winui:TextBox x:Name="LinkTextBox2" Margin="5" PlaceholderText="Enter link 2" />
            <winui:TextBox x:Name="LinkTextBox3" Margin="5" PlaceholderText="Enter link 3" />
            <winui:Button x:Name="ConvertButton" Content="Convert Links" Click="ConvertButton_Click" Margin="5" />
            <winui:TextBox x:Name="ResultTextBox1" Margin="0,10,0,0" IsReadOnly="True"/>
            <winui:TextBox x:Name="ResultTextBox2" Margin="0,10,0,0" IsReadOnly="True"/>
            <winui:TextBox x:Name="ResultTextBox3" Margin="0,10,0,0" IsReadOnly="True"/>
        </muxc:StackLayout>
    </Grid>
</Window>
